{
  "name": "react-typescript-postcss-webpack",
  "version": "1.0.0",
  "description": "webpack boilerplate for react",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "build:prod": "yarn run clean && webpack --config webpack.config.prod.ts --env.NODE_ENV='production'",
    "serve:prod": "webpack-dev-server --config webpack.config.prod.ts --port 3333 --env.NODE_ENV='production'",
    "build:dev": "yarn run clean && webpack --config webpack.config.dev.ts --env.NODE_ENV='development'",
    "serve:dev": "webpack-dev-server --config webpack.config.dev.ts --port 3333 --env.NODE_ENV='development'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "webpack"
  ],
  "author": "Alejandro Urdiales <alejandro.urdiales.93@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "9.6.1",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^5.0.19",
    "@types/redux-logger": "^3.0.6",
    "@types/redux-saga": "^0.10.5",
    "awesome-typescript-loader": "^5.0.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "css-mqpacker": "^6.0.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-font-magician": "^2.1.1",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.0",
    "stylelint-webpack-plugin": "^0.10.4",
    "tslint": "^5.10.0",
    "tslint-loader": "^3.6.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.8.3",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
